columns
filters
values
#concat oper=and, filt=filters

columns
no filters
values
#concat oper=or, filt=columns

columns
no filters
no values
#default

columns
filters
no values
#concat oper=and, filt=filters
#!exclude

no columns
filters
values
#gen columns
#concat oper=and, filt=columns

no columns
no filters
values
#gen columns
#concat oper=or, filt=columns

no columns
no filters
no values
#default

━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━
no columns && values => gen columns
filters => oper=and
no filters => oper=or
values => !concat
